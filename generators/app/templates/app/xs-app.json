{
<% if(html5repo && !managedAppRouter){ -%>
    "welcomeFile": "/<%= projectName %>html5/index.html",
<% if(authentication){ -%>
    "authenticationMethod": "route",
<% } else { -%>
    "authenticationMethod": "none",
<% } -%>
    "routes": []
<% } else { -%>
<% if(ui){ -%>
    "welcomeFile": "index.html",
<% } -%>
<% if(authentication){ -%>
    "authenticationMethod": "route",
<% } else { -%>
    "authenticationMethod": "none",
<% } -%>
    "routes": [
<% if(hana || api){ -%>
        {
            "source": "^/catalog/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srv"
        }
<% if(srv2){ -%>
        ,
        {
            "source": "^/catalog2/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srv2"
        }
<% } -%>
<% if(srvjs){ -%>
        ,
        {
            "source": "^/srvjs/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srvjs"
        }
<% } -%>
<% if(swagger){ -%>
        ,
        {
            "source": "^/swagger/catalog/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srv"
        }
<% } -%>
<% if(v2support){ -%>
        ,
        {
            "source": "^/v2/catalog/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srv"
        }
<% } -%>
<% if(schemaName !== '' && hanaTargetHDI === ''){ -%>
        ,
<% } -%>
<% if(hanaTargetHDI !== ''){ -%>
        ,
<% } -%>
<% } -%>
<% if(schemaName !== '' && hanaTargetHDI === ''){ -%>
        {
            "source": "^/<%= schemaName %>/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srv"
        }
<% } -%>
<% if((schemaName !== '' && hanaTargetHDI === '') && swagger){ -%>
        ,
        {
            "source": "^/swagger/<%= schemaName %>/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srv"
        }
<% } -%>
<% if((schemaName !== '' && hanaTargetHDI === '') && v2support){ -%>
        ,
        {
            "source": "^/v2/<%= schemaName %>/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srv"
        }
<% } -%>
<% if(hanaTargetHDI !== ''){ -%>
        {
            "source": "^/<%= hanaTargetHDI %>/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srv"
        }
<% } -%>
<% if(hanaTargetHDI !== '' && swagger){ -%>
        ,
        {
            "source": "^/swagger/<%= hanaTargetHDI %>/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srv"
        }
<% } -%>
<% if(hanaTargetHDI && v2support){ -%>
        ,
        {
            "source": "^/v2/<%= hanaTargetHDI %>/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srv"
        }
<% } -%>
<% if(multiTenant){ -%>
        ,
        {
            "source": "^/extend/(.*)",
            "target": "$1",
<% if(authentication){ -%>
            "authenticationType": "none",
<% } -%>
            "destination": "srv"
        }
<% } -%>
<% if(haa){ -%>
<% if(hana || api || (schemaName !== '' && hanaTargetHDI === '') || hanaTargetHDI !== '' || multiTenant){ -%>
        ,
<% } -%>
        {
            "source": "/sap/bc/ina",
            "httpMethods": ["POST", "GET"],
            "authenticationType": "xsuaa",
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "haa"
        },
        {
            "source": "/sap/bc/ina",
            "httpMethods": ["OPTIONS"],
            "authenticationType": "none",
            "destination": "haa"
        }
<% } -%>
<% if(ui){ -%>
<% if(hana || api || (schemaName !== '' && hanaTargetHDI === '') || hanaTargetHDI !== '' || multiTenant || haa){ -%>
        ,
<% } -%>
        {
            "source": "^/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "localDir": "resources"
        }
<% } -%>
    ]
<% } -%>
}