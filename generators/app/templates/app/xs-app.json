{
<% if(html5repo && !managedAppRouter){ -%>
    "welcomeFile": "/<%= projectName %>html5/index.html",
<% if(authentication){ -%>
    "authenticationMethod": "route",
<% } else { -%>
    "authenticationMethod": "none",
<% } -%>
    "routes": []
<% } else { -%>
<% if(ui){ -%>
    "welcomeFile": "index.html",
<% } -%>
<% if(authentication){ -%>
    "authenticationMethod": "route",
<% } else { -%>
    "authenticationMethod": "none",
<% } -%>
    "routes": [
<% if(hana || hanaTargetHDI !== "" || api){ -%>
        {
            "source": "^/catalog/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srv"
        }
<% if(v2support){ -%>
        ,
        {
            "source": "^/v2/catalog/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srv"
        }
<% } -%>
<% if(schemaName !== ''){ -%>
        ,
<% } -%>
<% } -%>
<% if(schemaName !== ''){ -%>
        {
            "source": "^/<%= schemaName %>/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srv"
        }
<% } -%>
<% if(schemaName !== '' && v2support){ -%>
        ,
        {
            "source": "^/v2/<%= schemaName %>/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "srv"
        }
<% } -%>
<% if(multiTenant){ -%>
        ,
        {
            "source": "^/extend/(.*)",
            "target": "$1",
<% if(authentication){ -%>
            "authenticationType": "none",
<% } -%>
            "destination": "srv"
        }
<% } -%>
<% if(haa){ -%>
<% if(hana || hanaTargetHDI !== "" || api || schemaName !== '' || multiTenant){ -%>
        ,
<% } -%>
        {
            "source": "/sap/bc/ina",
            "httpMethods": ["POST", "GET"],
            "authenticationType": "xsuaa",
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "destination": "haa"
        },
        {
            "source": "/sap/bc/ina",
            "httpMethods": ["OPTIONS"],
            "authenticationType": "none",
            "destination": "haa"
        }
<% } -%>
<% if(ui){ -%>
<% if(hana || hanaTargetHDI !== "" || api || schemaName !== '' || multiTenant || haa){ -%>
        ,
<% } -%>
        {
            "source": "^/(.*)$",
<% if(authentication){ -%>
            "authenticationType": "xsuaa",
<% } -%>
<% if(authorization){ -%>
            "scope": "$XSAPPNAME.Viewer",
<% } -%>
            "localDir": "resources"
        }
<% } -%>
    ]
<% } -%>
}